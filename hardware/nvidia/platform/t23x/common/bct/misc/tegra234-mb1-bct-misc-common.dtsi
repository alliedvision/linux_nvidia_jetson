                te990m { // SKU = 0xD0
                    vmin_threshold = <0x14>;
                    vmax_threshold = <0x7B>;
                };
                te980m { // SKU = 0xD3
                    vmin_threshold = <0x14>;
                    vmax_threshold = <0x7B>;
                };
                ta979sa { // SKU = 0x92
                    vmin_threshold = <0x17>;
                    vmax_threshold = <0x6D>;
                };
                te985m { // SKU = 0xD2
                    vmin_threshold = <0x14>;
                    vmax_threshold = <0x7B>;
                };
                te970m { // SKU = 0xD4
                    vmin_threshold = <0x14>;
                    vmax_threshold = <0x7B>;
                };
            };
        };

