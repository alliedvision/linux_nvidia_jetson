               coeff_fll_frug_fast = <0>;
               coeff_fll_frug_main = <0>;
               cfg2_fll_init = <0>;
               cfg2_fll_ctrl_ldmem = <0>;
               cfg2_fll_switch_ldmem = <0>;
               lut_sw_freq_req_sw_override_ndiv = <0>;
               lut_sw_freq_req_ndiv = <0>;
               cfg1_vfgain = <0>;
            };
        };

        ///////// Debug data //////////
        debug {
            uart_instance = <2>;
            wdt_period_secs = <0>;
#ifdef DISABLE_UART_MB1_MB2
            log_level = <0>;
#else
